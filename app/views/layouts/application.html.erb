<!DOCTYPE html>
<html>
  <head>
    <title>SmartBookify</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "application", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "account", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "admin", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "calendar", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "client_calender", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "client", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "dashboard", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "errors", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "forgot_password", "data-turbo-track": "reload" %>
    <%= stylesheet_link_tag "login", "data-turbo-track": "reload" %>
   
    <%= javascript_importmap_tags %>
    <%= javascript_include_tag 'custom/url' %>

    <%= turbo_include_tags %>

    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>

    <script rel="preload" href="client_navbar_script.js"  as="script" crossorigin="anonymous"></script>
    <style>
      @import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;700&display=swap');
    </style>
    
  </head>

  <body>
    <%= yield %>
  </body>
</html>
